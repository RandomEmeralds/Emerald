ChatBypass.Name = "ChatBypass"
ChatBypass.Parent = Scripts
ChatBypass.BackgroundColor3 = Color3.fromRGB(22, 22, 22)
ChatBypass.BorderColor3 = Color3.fromRGB(13, 13, 13)
ChatBypass.BorderSizePixel = 4
ChatBypass.Position = UDim2.new(0.0440000482, 0, 0.736363649, 0)
ChatBypass.Size = UDim2.new(0, 135, 0, 50)
ChatBypass.Font = Enum.Font.GothamBold
ChatBypass.Text = "Chat Bypass"
ChatBypass.TextColor3 = Color3.fromRGB(255, 255, 255)
ChatBypass.TextSize = 14.000
ChatBypass.MouseButton1Down:connect(function() 
--written by 0xPeep
--𝓽𝓱𝓪𝓽 𝓲𝓼 𝓿𝓮𝓻𝔂 𝓶𝓾𝓬𝓱 𝓪𝓭𝓮𝓺𝓾𝓪𝓽𝓮

local character1="ˌˌˌ";
local character2="  ";
local words=game:HttpGet("https://pastebin.com/1w4AzjzV",true);

local mt=getrawmetatable(game);
local namecall=mt.__namecall;
setreadonly(mt,false);

mt.__namecall=newcclosure(function(self,...)
    local params={...};
    if (self.Name=="SayMessageRequest") then
        params[1]=string.gsub(params[1]," ",character2);
        for word in params[1]:gmatch"%S+" do
            if (word:len()>3) and words:find(word:lower()) then
                params[1]=string.gsub(params[1],word,string.gsub(word,".",function(o)
                    return o..character1;
                end))
            end
        end
    end
    return namecall(self,unpack(params));
end)

setreadonly(mt,true);

warn"[V2]: Chat bypass loaded! Credits to 0xPeep"; 
end)

